@{
    Layout = "~/Areas/UserDashboard/Views/Shared/_Layout.cshtml";
}

@model RobloxWithPinoo_UI.Entity.Dtos.DocArticleDtos.DocArticleDetails

<div class="card">
    <div class="card-body">
        <div class="blog-single">
            <div class="blog-box blog-details">
                <img class="img-fluid w-100" src="@Model.ImageUrl" alt="blog-main">
                <div class="blog-details">
                    <br />
                    <h4 class="pt-2">@Model.Title</h4>
                    <div class="single-blog-content-top">

                            @Html.Raw(Model.Content)

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>